{
  "name": "ra-https",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf packages/*/dist/* packages/*/*.tsbuildinfo",
    "typecheck": "tsc --build --watch",
    "build": "npm --workspaces run build",
    "lint": "eslint .",
    "dev": "npm --workspace packages/demo run dev",
    "preview": "npm --workspace packages/demo run preview",
    "server": "npm --workspace packages/demo run server",
    "start": "npm --workspace packages/demo run start",
    "test": "npm --workspaces run test"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "tsx": "^4.20.5"
  },
  "devDependencies": {
    "process": "^0.11.10"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "nodeArguments": [
      "--import=tsx"
    ],
    "files": [
      "test/**/*.test.ts"
    ]
  }
}
